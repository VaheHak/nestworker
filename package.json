{
  "name": "nestworker",
  "version": "1.0.25",
  "description": "A lightweight tool built on top of Node.js worker_threads, enabling multithreading.",
  "author": "Vahe Hakobyan",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepublish": "npm run build"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "engines": {
    "node": ">=14.17.0"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "node",
    "express",
    "worker",
    "workers",
    "thread",
    "worker_threads",
    "multithreading"
  ],
  "bugs": {
    "url": "https://github.com/VaheHak/nestworker/issues"
  },
  "homepage": "https://github.com/VaheHak/nestworker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VaheHak/nestworker.git"
  },
  "dependencies": {
    "@nestjs/common": "10.3.8"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4.5"
  }
}
